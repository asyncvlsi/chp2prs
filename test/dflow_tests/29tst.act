import "29.act";
import "29_df_chp.act";

defproc test()
{
  chan(int) X;
  chan(int) Y;
  
  df_testproc d(X,Y);

  int a, b, c;
  int i;
  bool success;

  chp {
      i := 0;
      success+;
    X!15;Y?a;
    log ("step 1");
    X!18;Y?b;
    log ("step 2");
    [ a = 5 & b = 10 -> skip [] else -> success-; log ("FAILURE: ", a, " b=", b) ];
   *[  i < 10 ->
    X!15;Y?a;
    [ a = 8 -> skip [] else -> success-; log ("FAILURE: ", a) ];
    i := i + 1
    ];
    [ success -> log ("SUCCESS") [] else -> skip ]
  }
}
