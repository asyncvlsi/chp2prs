import "5decomp.act";

defproc test ()
{
    chan(int) X1, X2, Y1, Y2, Y3;
    decomp_le5 m(X1, X2, Y1, Y2, Y3);

    int<16> y11, y12, y2, y3;

    chp {
        ( ( X1!1; X1!2; X1!3; X1!4 ) , (X2!3) , (Y1?y11; Y1?y12) , Y3?y3 ) ;
        [ y11=3 & y12=5 & y3=3 -> log ("SUCCESS") 
        [] else -> log ("FAILURE") 
        ]
    }
}
